{
  "compilerOptions": {
      "target": "ESNEXT",
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": ".",
      "moduleResolution": "node",
      "baseUrl": "./",
      "paths": {
        "@/*": ["src/*"]
      },
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "skipLibCheck": true
  }
}
